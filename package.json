{
  "name": "sustainable-browser-extension",
  "version": "0.0.4",
  "description": "A browser extension that intercepts CDN requests and serves local dependencies",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "analyze:deps": "ts-node --project tsconfig.scripts.json src/scripts/analyze-dependencies.ts",
    "download:deps": "yarn analyze:deps && ts-node --project tsconfig.scripts.json src/scripts/download-dependencies.ts",
    "process:imports": "ts-node --project tsconfig.scripts.json src/scripts/postprocessing/importTransforming.ts",
    "process:import-mapping": "ts-node --project tsconfig.scripts.json src/scripts/postprocessing/relativeImportMapping.ts",
    "export:deps": "ts-node --project tsconfig.scripts.json src/scripts/export-dependencies.ts",
    "build-cross-browser": "rimraf dist && ts-node --project tsconfig.scripts.json scripts/build-cross-browser.ts",
    "build-webstore": "rimraf dist && ts-node --project tsconfig.scripts.json scripts/build-cross-browser.ts --webstore",
    "dev": "yarn build:watch",
    "clean": "rm -rf dist",
    "process:deps": "yarn process:import-mapping && yarn process:imports",
    "setup": "yarn && yarn download:deps && yarn process:deps"
  },
  "keywords": [
    "browser-extension",
    "cdn",
    "dependencies",
    "sustainable"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/parser": "^7.28.4",
    "@babel/traverse": "^7.28.4",
    "@babel/types": "^7.28.4",
    "@types/chrome": "^0.0.268",
    "@types/jest": "^29.5.5",
    "@types/node": "^24.7.0",
    "@types/semver": "^7.5.2",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "semver": "^7.7.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "axios": "^1.5.0"
  }
}
